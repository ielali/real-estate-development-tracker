{
  "name": "realestate-portfolio",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.2.23",
  "description": "A comprehensive web application for managing real estate development projects, tracking costs, managing contacts, and providing transparent partner dashboards.",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "test": "turbo run test",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ielali/real-estate-development-tracker.git"
  },
  "keywords": [
    "real-estate",
    "project-management",
    "next.js",
    "typescript",
    "turborepo"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ielali/real-estate-development-tracker/issues"
  },
  "homepage": "https://github.com/ielali/real-estate-development-tracker#readme",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "eslint": "^8.57.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "trustedDependencies": [
    "@realestate-portfolio/web",
    "es5-ext",
    "unrs-resolver"
  ],
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "better-auth": "^1.3.13",
    "drizzle-orm": "^0.44.6",
    "recharts": "^3.2.1",
    "ws": "^8.18.3"
  }
}
